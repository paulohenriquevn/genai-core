# GenAI Core - Exemplo de arquivo de variáveis de ambiente
# Copie este arquivo para .env e configure as variáveis conforme necessário

# API Keys para serviços de IA
OPENAI_API_KEY=sua_chave_openai_aqui
ANTHROPIC_API_KEY=sua_chave_anthropic_aqui
GOOGLE_API_KEY=sua_chave_google_aqui
HF_API_KEY=sua_chave_huggingface_aqui

# Configurações de banco de dados
POSTGRES_URL=postgresql://postgres:postgres@localhost:5432/genai
DUCKDB_PATH=:memory:

# Configurações da aplicação
LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR, CRITICAL
ENVIRONMENT=development  # development, staging, production

# Configurações de cache
CACHE_ENABLED=True
CACHE_TTL=3600  # Tempo em segundos
